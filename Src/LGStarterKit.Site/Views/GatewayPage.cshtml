@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
	Layout = "Design.cshtml";
}

<div class="row">
    <div class="col-xs-12">
        <h2>@Umbraco.Field("title")</h2>
        @CurrentPage.GetGridHtml("content", "lgsk-nested-bootstrap")
    </div>
</div>

@Html.Partial("Nav/GatewayLinks", Model.Content,
    new ViewDataDictionary()
    {
        { "mainCount", Umbraco.Field("mainItemCount", altText: "6") }
    })
